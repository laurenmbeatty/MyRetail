{
  "name": "myretail",
  "version": "1.0.0",
  "description": "test cast for Target Engineering",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,html,json}\"",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "test": "jest --silent",
    "test:update": "jest --silent -u",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "deploy": "npm run build  && surge dist my-retail.surge.sh"
  },
  "author": "Lauren Beatty",
  "license": "ISC",
  "devDependencies": {
    "@emotion/core": "^10.0.1",
    "@emotion/styled": "^10.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "emotion": "^10.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "nuka-carousel": "^4.4.4",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.15.2",
    "react-emotion": "^10.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./src/setupTests.js",
    "setupFiles": [
      "./src/setupTests.js"
    ]
  }
}
